            var chk10 = $('#chk10').is(':checked');
            var chk11 = $('#chk11').is(':checked');
            var chk12 = $('#chk12').is(':checked');
            var arrayprod = [chk1, chk2, chk3, chk4, chk5, chk6, chk7, chk8, chk9, chk10, chk11, chk12];

            var chkotro = $('#txotro').val();
            var txnombre = $('#txnombre').val();
            var slcDepartamento = $('#slcde option').filter(':selected').val();
            var txCorreo = $('#txemail').val();

            if (txnombre == "" || txCorreo == "") {
                alert("Por favor completar los campos");
            } else {
                $.ajax({
                    url: vgruta + "landing/enviarCompra",
                    cache: false,
                    type: "POST",
                    data: {
                        "wchk": arrayprod.toString(),
                        "wotro": chkotro,
                        "wnombre": txnombre,
                        "widepartamento": slcDepartamento,
                        "wcorreo": txCorreo
