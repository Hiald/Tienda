        function enviarVenta() {

            var txContacto = $('#txnombrec').val();
            var txCelular = $('#txcelular').val();
            var slcDepartamento = $('#slcde option').filter(':selected').val();
            var txCorreoVenta = $('#txcorreoVenta').val();

            if (txContacto === "" || slcDepartamento === "0" || txCorreoVenta === "") {
                alert("Por favor completar los campos");
            } else {

                var testEmail = /^[A-Z0-9._%+-]+@@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
                if (testEmail.test(txCorreoVenta)) {
                    $.ajax({
                        url: vgruta + "landing/enviarVenta",
                        cache: false,
                        type: "POST",
                        data: {
                            "wtienda": "",
                            "wcontacto": txContacto,
                            "wcelular": txCelular,
                            "witiponegocio": 1,
                            "wruc": "",
                            "wdireccion": "",
                            "widepartamento": slcDepartamento,
                            "wcorreoventa": txCorreoVenta
                        },
                        success: function (respuesta) {
                            console.log(respuesta);
                            $('#mventa').modal('hide');
                            alert("Muchas gracias, nos pondremos en contacto contigo lo más pronto posible");

                            $('#txnombrec').val("");
                            $('#txcelular').val("");
                            $('#txcorreoVenta').val("");
                        },
                        error: function () {
                            console.log("error al registrar");
                        }
                    });
                } else {
                    alert("Por favor ingrese un correo válido");
                }

            }

        }

