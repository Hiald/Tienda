        $(document).ready(function () {
            var ivalorEstado = $('#hddVentaId').attr('data-sestado');

            if (ivalorEstado === "Esperando tiempo de envío") {
                $('#tabpaso0').addClass("active");
                $('#txEstado').html("Estado: Esperando tiempo de envío");
            } else if (ivalorEstado === "Procesando orden en camino") {
                $('#tabpaso0').addClass("completed");
                $('#tabpaso1').addClass("active");
                $('#txEstado').html("Estado: Procesando orden en camino");
            } else if (ivalorEstado === "Entregando en domicilio") {
                $('#tabpaso0').addClass("completed");
                $('#tabpaso1').addClass("completed");
                $('#tabpaso2').addClass("active");
                $('#txEstado').html("Estado: Entregando en domicilio");
            } else if (ivalorEstado === "Orden finalizada") {
                $('#tabpaso0').addClass("completed");
                $('#tabpaso1').addClass("completed");
                $('#tabpaso2').addClass("completed");
                $('#tabpaso3').addClass("active");
                $('#txEstado').html("Estado: Orden finalizada");
            } else if (ivalorEstado === "Reprogramando el tiempo de llegada") {
                $('#tabpaso0').addClass("completed");
