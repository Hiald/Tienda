    <!-- Order Details Modal-->
    <div class="modal fade" id="order-details">
        <div class="modal-dialog modal-lg modal-dialog-scrollable">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Orden código: @ViewBag.GEnvioId</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body pb-0">
                    @if (Enumerable.Count(ViewBag.rDatoSV) > 0)
                    {
                        decimal dsubtotal = 0;
                        foreach (var item in ViewBag.rDatoSV)
                        {
                            dsubtotal = dsubtotal + item.dsubtotal;
                            int surl = item.sproductoimagen1.Length - 1;
                            <div class="d-sm-flex justify-content-between mb-4 pb-3 pb-sm-2 border-bottom">
                                <div class="media d-block d-sm-flex text-center text-sm-left">
                                    <a class="d-inline-block mx-auto mr-sm-4" style="width: 10rem;"
                                       @*href="@Url.Action("detalle", "index", new { wtipoPromocion = item.ipromociontipo, wproductoid = item.productoid })"*@>
                                        <img src="@item.sproductoimagen1.ToString().Substring(1, surl)" alt="Producto" style="height:140px;width:150px;">
                                    </a>
                                    <div class="media-body pt-2">
                                        <h3 class="product-title font-size-base mb-2"><a>@item.sproductonombre</a></h3>
                                    </div>
                                </div>

                                <div class="pt-2 pl-sm-3 mx-auto mx-sm-0 text-center">
                                    <div class="text-muted mb-2">Cantidad:</div>@item.icantidad
                                </div>
                                <div class="pt-2 pl-sm-3 mx-auto mx-sm-0 text-center">
                                    <div class="text-muted mb-2">Subtotal</div>S/. @item.dsubtotal
                                </div>
                            </div>
                        }
                        ViewBag.dsubtotal = dsubtotal;
                    }
                </div>
                <!-- pie del modal detalles del pago -->
                <div class="modal-footer flex-wrap justify-content-between bg-secondary font-size-md">
                    @if (Enumerable.Count(ViewBag.rDatoV) > 0)
                    {
                        foreach (var item in ViewBag.rDatoV)
                        {
                            <div class="px-2 py-1"><span class="text-muted">Sub Total :&nbsp;</span><span>S/. @ViewBag.dsubtotal </span></div>
                            <div class="px-2 py-1"><span class="text-muted">Costo de envío :&nbsp;</span><span>S/. @item.dcomision</span></div>
                            <div class="px-2 py-1"><span class="text-muted">Total :&nbsp;</span><span>S/. @item.dtotal</span></div>
                        }
                    }
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
